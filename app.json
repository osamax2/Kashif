{
  "expo": {
    "name": "Kashif",
    "slug": "kashif-app",
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kashifapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "privacy": "public",
    "extra": {
      "privacyPolicyUrl": "https://admin.kashifroad.com/privacy",
